{
  "kind": "collectionType",
  "collectionName": "addresses",
  "info": {
    "singularName": "address",
    "pluralName": "addresses",
    "displayName": "Address"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string"
    },
    "MobileNumber": {
      "type": "biginteger",
      "max": "10"
    },
    "Email": {
      "type": "email"
    },
    "Country": {
      "type": "string"
    },
    "Address_line_1": {
      "type": "string"
    },
    "Address_line_2": {
      "type": "string"
    },
    "City": {
      "type": "string"
    },
    "State": {
      "type": "string"
    },
    "Postal_code": {
      "type": "string"
    },
    "register": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::register.register",
      "inversedBy": "addresses"
    }
  }
}
