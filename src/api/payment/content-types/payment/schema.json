{
  "kind": "collectionType",
  "collectionName": "payments",
  "info": {
    "singularName": "payment",
    "pluralName": "payments",
    "displayName": "Payment"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "payment_id": {
      "type": "string"
    },
    "order_id": {
      "type": "string"
    },
    "Amount": {
      "type": "biginteger",
      "min": "100"
    },
    "Condition": {
      "type": "enumeration",
      "enum": [
        "Created",
        "Paid",
        "Pending"
      ]
    },
    "register": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::register.register",
      "inversedBy": "payments"
    },
    "cart": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::cart.cart",
      "mappedBy": "payment"
    }
  }
}
