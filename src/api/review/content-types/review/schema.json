{
  "kind": "collectionType",
  "collectionName": "reviews",
  "info": {
    "singularName": "review",
    "pluralName": "reviews",
    "displayName": "Review"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Review": {
      "type": "string"
    },
    "Rating": {
      "type": "integer",
      "min": 0,
      "max": 5
    },
    "ReviewImage": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "register": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::register.register",
      "inversedBy": "reviews"
    },
    "product": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::product.product",
      "inversedBy": "reviews"
    }
  }
}
